{
  "name": "grpc-methods",
  "version": "0.7.0",
  "description": "Method wrappers for Grpc to include logging and promises",
  "main": "src/index.js",
  "scripts": {
    "format": "standard src --fix",
    "test": "npm run format && NODE_PATH=. nyc mocha 'src/**/*.spec.js'",
    "generate-docs": "node_modules/.bin/jsdoc --configure .jsdoc.json --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kinesis-exchange/grpc-method.git"
  },
  "keywords": [
    "grpc",
    "promise"
  ],
  "standard": {
    "env": [
      "mocha",
      "chai"
    ]
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js"
    ]
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kinesis-exchange/grpc-method/issues"
  },
  "homepage": "https://github.com/kinesis-exchange/grpc-method#readme",
  "dependencies": {
    "grpc": "1.15.1"
  },
  "peerDependencies": {
    "grpc": "1.15.x"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "dirty-chai": "2.0.1",
    "jsdoc": "3.5.5",
    "minami": "1.2.3",
    "mocha": "5.0.5",
    "nyc": "13.2.0",
    "rewire": "4.0.1",
    "sinon": "7.2.3",
    "sinon-chai": "3.3.0",
    "standard": "11.0.1",
    "timekeeper": "2.1.2",
    "timeout-as-promise": "1.0.0"
  }
}
